{
    "name": "devlin/laravel-model-analyzer",
    "description": "Analyze and validate Eloquent relationships against database schema",
    "keywords": [
        "laravel",
        "eloquent",
        "relationships",
        "database",
        "analyzer",
        "linter",
        "static-analysis"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Devlin",
            "email": "devlin@example.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/database": "^8.0",
        "symfony/finder": "^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Devlin\\ModelAnalyzer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devlin\\ModelAnalyzer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devlin\\ModelAnalyzer\\ModelAnalyzerServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "audit": {
            "block-insecure": false
        }
    }
}
